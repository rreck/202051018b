{
  "dashboard": {
    "title": "CrewAI ACH Fraud Detection Agent",
    "tags": ["crewai", "fraud-detection", "ach", "financial"],
    "timezone": "browser",
    "editable": true,
    "graphTooltip": 0,
    "panels": [
      {
        "id": 1,
        "gridPos": {"h": 6, "w": 6, "x": 0, "y": 0},
        "title": "Files Processed",
        "type": "stat",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "fraud_ach_files_processed_total",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "gridPos": {"h": 6, "w": 6, "x": 6, "y": 0},
        "title": "Transactions Analyzed",
        "type": "stat",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "fraud_ach_transactions_analyzed_total",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "blue", "value": null}
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "gridPos": {"h": 6, "w": 6, "x": 12, "y": 0},
        "title": "Fraud Detected",
        "type": "stat",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "fraud_ach_fraud_detected_total",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": null}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "gridPos": {"h": 6, "w": 6, "x": 18, "y": 0},
        "title": "Fraud Detection Rate",
        "type": "gauge",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "rate(fraud_ach_fraud_detected_total[5m]) * 60",
            "refId": "A",
            "legendFormat": "Fraud/min"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 10},
                {"color": "orange", "value": 25},
                {"color": "red", "value": 50}
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 6},
        "title": "Fraud by Type",
        "type": "piechart",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "fraud_ach_fraud_by_type",
            "refId": "A",
            "legendFormat": "{{type}}"
          }
        ]
      },
      {
        "id": 6,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 6},
        "title": "Processing Over Time",
        "type": "graph",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "rate(fraud_ach_transactions_analyzed_total[5m]) * 60",
            "refId": "A",
            "legendFormat": "Transactions/min"
          },
          {
            "expr": "rate(fraud_ach_fraud_detected_total[5m]) * 60",
            "refId": "B",
            "legendFormat": "Fraud/min"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Transactions/min"},
          {"format": "short"}
        ]
      },
      {
        "id": 7,
        "gridPos": {"h": 6, "w": 8, "x": 0, "y": 14},
        "title": "Memory Artifacts Created",
        "type": "stat",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "fraud_ach_memory_artifacts_total",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "purple", "value": null}
              ]
            }
          }
        }
      },
      {
        "id": 8,
        "gridPos": {"h": 6, "w": 8, "x": 8, "y": 14},
        "title": "Agent Uptime",
        "type": "stat",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "fraud_ach_daemon_uptime_seconds",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null}
              ]
            }
          }
        }
      },
      {
        "id": 9,
        "gridPos": {"h": 6, "w": 8, "x": 16, "y": 14},
        "title": "Failed Files",
        "type": "stat",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "fraud_ach_files_failed_total",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "orange", "value": null}
              ]
            }
          }
        }
      }
    ],
    "refresh": "30s",
    "time": {"from": "now-1h", "to": "now"}
  }
}
