{
  "dashboard": {
    "title": "CrewAI Fraud Simulation - Card Testing Detection",
    "tags": ["crewai", "fraud-simulation", "card-testing", "synthetic-data"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "gridPos": {"x": 0, "y": 0, "w": 6, "h": 4},
        "title": "Total Runs",
        "type": "stat",
        "targets": [
          {
            "expr": "fraud_sim_runs_total",
            "legendFormat": "Total Runs"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "blue", "value": null}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "gridPos": {"x": 6, "y": 0, "w": 6, "h": 4},
        "title": "Runs In Progress",
        "type": "stat",
        "targets": [
          {
            "expr": "fraud_sim_runs_in_progress",
            "legendFormat": "In Progress"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "orange", "value": 3}
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "gridPos": {"x": 12, "y": 0, "w": 6, "h": 4},
        "title": "Completed Runs",
        "type": "stat",
        "targets": [
          {
            "expr": "fraud_sim_runs_completed",
            "legendFormat": "Completed"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "gridPos": {"x": 18, "y": 0, "w": 6, "h": 4},
        "title": "Failed Runs",
        "type": "stat",
        "targets": [
          {
            "expr": "fraud_sim_runs_failed",
            "legendFormat": "Failed"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "red", "value": 1}
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "gridPos": {"x": 0, "y": 4, "w": 12, "h": 8},
        "title": "Detection Performance Metrics",
        "type": "graph",
        "targets": [
          {
            "expr": "fraud_sim_last_run_accuracy",
            "legendFormat": "Accuracy"
          },
          {
            "expr": "fraud_sim_last_run_precision",
            "legendFormat": "Precision"
          },
          {
            "expr": "fraud_sim_last_run_recall",
            "legendFormat": "Recall"
          },
          {
            "expr": "fraud_sim_last_run_f1_score",
            "legendFormat": "F1 Score"
          }
        ],
        "yaxes": [
          {
            "format": "percentunit",
            "min": 0,
            "max": 1
          }
        ]
      },
      {
        "id": 6,
        "gridPos": {"x": 12, "y": 4, "w": 12, "h": 8},
        "title": "Pattern Flags Detected",
        "type": "bargauge",
        "targets": [
          {
            "expr": "fraud_sim_pattern_flags",
            "legendFormat": "{{flag}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 10},
                {"color": "red", "value": 50}
              ]
            }
          }
        },
        "options": {
          "orientation": "horizontal",
          "displayMode": "gradient"
        }
      },
      {
        "id": 7,
        "gridPos": {"x": 0, "y": 12, "w": 8, "h": 6},
        "title": "Last Run Accuracy",
        "type": "gauge",
        "targets": [
          {
            "expr": "fraud_sim_last_run_accuracy",
            "legendFormat": "Accuracy"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0,
            "max": 1,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 0.7},
                {"color": "green", "value": 0.9}
              ]
            }
          }
        }
      },
      {
        "id": 8,
        "gridPos": {"x": 8, "y": 12, "w": 8, "h": 6},
        "title": "Last Run Precision",
        "type": "gauge",
        "targets": [
          {
            "expr": "fraud_sim_last_run_precision",
            "legendFormat": "Precision"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0,
            "max": 1,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 0.7},
                {"color": "green", "value": 0.9}
              ]
            }
          }
        }
      },
      {
        "id": 9,
        "gridPos": {"x": 16, "y": 12, "w": 8, "h": 6},
        "title": "Last Run Recall",
        "type": "gauge",
        "targets": [
          {
            "expr": "fraud_sim_last_run_recall",
            "legendFormat": "Recall"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0,
            "max": 1,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 0.7},
                {"color": "green", "value": 0.9}
              ]
            }
          }
        }
      },
      {
        "id": 10,
        "gridPos": {"x": 0, "y": 18, "w": 12, "h": 6},
        "title": "Total Transactions Analyzed",
        "type": "stat",
        "targets": [
          {
            "expr": "fraud_sim_total_transactions_analyzed",
            "legendFormat": "Transactions"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "blue", "value": null}
              ]
            }
          }
        }
      },
      {
        "id": 11,
        "gridPos": {"x": 12, "y": 18, "w": 12, "h": 6},
        "title": "Total Fraud Detected",
        "type": "stat",
        "targets": [
          {
            "expr": "fraud_sim_total_fraud_detected",
            "legendFormat": "Fraud Detected"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "orange", "value": null}
              ]
            }
          }
        }
      },
      {
        "id": 12,
        "gridPos": {"x": 0, "y": 24, "w": 24, "h": 8},
        "title": "Run Completion Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(fraud_sim_runs_completed[5m])",
            "legendFormat": "Completions/min"
          },
          {
            "expr": "rate(fraud_sim_runs_failed[5m])",
            "legendFormat": "Failures/min"
          }
        ]
      }
    ]
  }
}
